SELECT -LogLikelihood/5 as LOG,LogDiff/100 as LR ,ELDLogLikelihood as ELD, ELDPrior as FD, MID as absLR, class
FROM   imdb_MovieLens_Drama_Sep29_00.OutlierDetector order by ELDLogLikelihood desc ; -- sum

