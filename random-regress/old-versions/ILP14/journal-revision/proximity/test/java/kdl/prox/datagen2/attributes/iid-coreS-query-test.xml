<!DOCTYPE graph-query SYSTEM "graph-query.dtd">

<graph-query name="New Query">
    <description>New Editor Query</description>
    <query-body>
        <vertex name="T">
            <condition>
                <test>
                    <operator>eq</operator>
                    <attribute-name>objecttype</attribute-name>
                    <value>T</value>
                </test>
            </condition>
            <numeric-annotation>
                <min>0</min>
            </numeric-annotation>
        </vertex>
        <vertex name="S">
            <condition>
                <test>
                    <operator>eq</operator>
                    <attribute-name>objecttype</attribute-name>
                    <value>S</value>
                </test>
            </condition>
        </vertex>
        <edge name="linkedTo">
            <vertex1>S</vertex1>
            <vertex2>T</vertex2>
            <directed>true</directed>
            <numeric-annotation>
                <min>1</min>
            </numeric-annotation>
        </edge>
    </query-body>
    <editor-data>
        <vertex-location name="T" x="543" y="255"/>
        <vertex-location name="S" x="231" y="215"/>
    </editor-data>
</graph-query>