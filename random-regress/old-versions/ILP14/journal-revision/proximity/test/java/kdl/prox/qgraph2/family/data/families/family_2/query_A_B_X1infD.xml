<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graph-query SYSTEM "graph-query.dtd">
<graph-query name="_A_B_X1infD">
    <description>_A_B_X1infD</description>
    <query-body>
        <vertex name="A">
            <condition>
                <test>
                    <operator>eq</operator>
                    <attribute-name>objectType</attribute-name>
                    <value>A</value>
                </test>
            </condition>
        </vertex>
        <vertex name="B">
            <condition>
                <test>
                    <operator>eq</operator>
                    <attribute-name>objectType</attribute-name>
                    <value>B</value>
                </test>
            </condition>
        </vertex>
        <edge name="X">
            <vertex1>A</vertex1>
            <vertex2>B</vertex2>
            <directed>true</directed>
            <condition>
                <test>
                    <operator>eq</operator>
                    <attribute-name>linkType</attribute-name>
                    <value>X</value>
                </test>
            </condition>
            <numeric-annotation>
                <min>1</min>
            </numeric-annotation>
        </edge>
    </query-body>
</graph-query>

