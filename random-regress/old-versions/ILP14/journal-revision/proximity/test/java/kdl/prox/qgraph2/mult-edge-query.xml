<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graph-query SYSTEM "graph-query.dtd">

<graph-query name="Multiple Edge Query">
  <description>a test query with multiple edges and self-loops</description>
  <query-body>
    <vertex name="Vertex1" />
    <vertex name="Vertex2" />
    <vertex name="Vertex3" />
    <edge name="Edge1">
      <vertex1>Vertex1</vertex1>
      <vertex2>Vertex1</vertex2>
      <directed>true</directed>
      <numeric-annotation>
        <min>1</min>
      </numeric-annotation>
    </edge>
    <edge name="Edge2">
      <vertex1>Vertex1</vertex1>
      <vertex2>Vertex2</vertex2>
      <directed>true</directed>
      <numeric-annotation>
        <min>1</min>
      </numeric-annotation>
    </edge>
    <edge name="Edge3">
      <vertex1>Vertex2</vertex1>
      <vertex2>Vertex3</vertex2>
      <directed>true</directed>
      <numeric-annotation>
        <min>1</min>
      </numeric-annotation>
    </edge>
    <edge name="Edge4">
      <vertex1>Vertex2</vertex1>
      <vertex2>Vertex3</vertex2>
      <directed>true</directed>
      <numeric-annotation>
        <min>1</min>
      </numeric-annotation>
    </edge>
  </query-body>
</graph-query>

