<graph-query name="Basics_DB01_Q01">
  <description>Simple two-object query</description>
  <query-body>
    <vertex name="B" />
    <vertex name="A" />
    <edge name="X">
      <vertex1>A</vertex1>
      <vertex2>B</vertex2>
      <directed>true</directed>
    </edge>
  </query-body>
  <editor-data>
    <vertex-location name="B" x="351.0" y="122.0" />
    <vertex-location name="A" x="161.0" y="121.0" />
  </editor-data>
</graph-query>

