<graph-query name="Basics_DB02_Q01">
  <description>Simple three-object query</description>
  <query-body>
    <vertex name="A" />
    <vertex name="B" />
    <vertex name="C" />
    <edge name="X">
      <vertex1>A</vertex1>
      <vertex2>C</vertex2>
      <directed>true</directed>
    </edge>
    <edge name="Y">
      <vertex1>B</vertex1>
      <vertex2>C</vertex2>
      <directed>true</directed>
    </edge>
  </query-body>
  <editor-data>
    <vertex-location name="A" x="167.0" y="54.0" />
    <vertex-location name="B" x="166.0" y="147.0" />
    <vertex-location name="C" x="328.0" y="110.0" />
  </editor-data>
</graph-query>

